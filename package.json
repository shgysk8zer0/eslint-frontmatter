{
  "name": "eslint-plugin-frontmatter2",
  "version": "1.0.0",
  "description": "Plugin to allow YAML frontmatter to be ignored by ESLint",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-frontmatter",
    "eslint-plugin-frontmatter",
    "frontmatter",
    "jekyll",
    "liguid",
    "eleventy",
    "11ty"
  ],
  "type": "module",
  "main": "./plugin.cjs",
  "module": "./plugin.mjs",
  "exports": {
    ".": {
      "import": "./plugin.mjs",
      "require": "./plugin.cjs"
    },
    "./*.js": {
      "import": "./*.mjs",
      "require": "./*.cjs"
    },
    "./*.mjs": {
      "import": "./*.mjs",
      "require": "./*.cjs"
    },
    "./*.cjs": {
      "import": "./*.mjs",
      "require": "./*.cjs"
    },
    "./*": {
      "import": "./*.mjs",
      "require": "./*.cjs"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": false,
  "scripts": {
    "test": "npm run lint:js && npm run build",
    "preversion": "npm test",
    "prepare": "npm run build",
    "lint:js": "eslint .",
    "fix:js": "eslint . --fix",
    "build": "npm run build:js",
    "clean": "rm -f ./*.cjs && rm -f ./*.mjs",
    "build:js": "npm run clean && rollup -c rollup.config.js",
    "create:lock": "npm i --package-lock-only --ignore-scripts --no-audit --no-fund",
    "version:bump": "npm run version:bump:patch",
    "version:bump:patch": "npm version --no-git-tag-version patch && npm run create:lock",
    "version:bump:minor": "npm version --no-git-tag-version minor && npm run create:lock",
    "version:bump:major": "npm version --no-git-tag-version major && npm run create:lock"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shgysk8zer0/eslint-frontmatter.git"
  },
  "author": "Chris Zuber <admin@kernvalley.us>",
  "license": "MIT",
  "funding": [
    {
      "type": "librepay",
      "url": "https://liberapay.com/shgysk8zer0"
    },
    {
      "type": "github",
      "url": "https://github.com/sponsors/shgysk8zer0"
    }
  ],
  "bugs": {
    "url": "https://github.com/shgysk8zer0/eslint-frontmatter/issues"
  },
  "homepage": "https://github.com/shgysk8zer0/eslint-frontmatter#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "eslint": "^10.0.2",
    "rollup": "^4.14.3"
  }
}
